<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.henu.papermanage.common.system.dao.OauthAccessTokenMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.henu.papermanage.common.system.model.OauthAccessToken">
		<id column="authentication_id" property="authenticationId" />
		<result column="token_id" property="tokenId" />
		<result column="token" property="token" />
		<result column="user_name" property="userName" />
		<result column="client_id" property="clientId" />
		<result column="authentication" property="authentication" />
		<result column="refresh_token" property="refreshToken" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        token_id AS tokenId, token, authentication_id AS authenticationId, user_name AS userName, client_id AS clientId, authentication, refresh_token AS refreshToken
    </sql>

</mapper>
