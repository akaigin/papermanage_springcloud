<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.henu.papermanage.common.system.dao.PapReportMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.henu.papermanage.common.system.model.PapReport">
		<id column="id" property="id" />
		<result column="description" property="description" />
		<result column="file_name" property="fileName" />
		<result column="file_path" property="filePath" />
		<result column="create_time" property="createTime" />
		<result column="modify_time" property="modifyTime" />
		<result column="status" property="status" />
		<result column="guidance" property="guidance" />
		<result column="create_user" property="createUser" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, description, file_name AS fileName, file_path AS filePath, create_time AS createTime, modify_time AS modifyTime, status, guidance, create_user AS createUser
    </sql>

</mapper>
