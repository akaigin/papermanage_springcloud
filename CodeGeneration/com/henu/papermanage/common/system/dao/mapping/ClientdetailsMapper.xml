<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.henu.papermanage.common.system.dao.ClientdetailsMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.henu.papermanage.common.system.model.Clientdetails">
		<id column="appId" property="appId" />
		<result column="resourceIds" property="resourceIds" />
		<result column="appSecret" property="appSecret" />
		<result column="scope" property="scope" />
		<result column="grantTypes" property="grantTypes" />
		<result column="redirectUrl" property="redirectUrl" />
		<result column="authorities" property="authorities" />
		<result column="access_token_validity" property="accessTokenValidity" />
		<result column="refresh_token_validity" property="refreshTokenValidity" />
		<result column="additionalInformation" property="additionalInformation" />
		<result column="autoApproveScopes" property="autoApproveScopes" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        appId, resourceIds, appSecret, scope, grantTypes, redirectUrl, authorities, access_token_validity AS accessTokenValidity, refresh_token_validity AS refreshTokenValidity, additionalInformation, autoApproveScopes
    </sql>

</mapper>
